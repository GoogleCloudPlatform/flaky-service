{
  "hosting": {
    "site": "flaky-dashboard",
    "public": "dist/flaky-dashboard",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/home",
      "destination": "/index.html"
    }, {
      "source": "/api",
      "run": {
        "serviceId": "api",  
        "region": "us-central1"     
      }
    }, {
      "source" : "/api/**",
      "run" : {
        "serviceId" : "api",
        "region": "us-central1"
      }
    }],
    "cleanUrls": true
  }
}
