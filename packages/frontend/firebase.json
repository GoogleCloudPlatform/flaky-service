{
  "hosting": {
    "site": "flaky-dashboard",
    "public": "dist/flaky-dashboard",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/api",
      "run": {
        "serviceId": "api",  
        "region": "us-central1"     
      }
    }, {
      "source": "/home",
      "destination": "/index.html"
    }],
    "cleanUrls": true
  }
}
