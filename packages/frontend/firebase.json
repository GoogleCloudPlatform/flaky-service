{
  "hosting": {
    "site": "flaky-dev-staging",
    "public": "dist/flaky-dashboard",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source" : "/api/**",
      "run" : {
        "serviceId" : "api-staging",
        "region": "us-central1"
        }
      }, {
      "source": "/**",
      "destination": "/index.html"
      }, {
      "source": "/api",
      "run": {
        "serviceId": "api-staging",  
        "region": "us-central1"     
        }
      }],
    "cleanUrls": true
  }
}
