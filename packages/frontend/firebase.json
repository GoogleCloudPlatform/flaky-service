{
  "hosting": {
    "site": "flaky-dashboard",
    "public": "dist/flaky-dashboard",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "run": {
        "serviceId": "flaky-api",  
        "region": "us-central1"     
      }
    }]
  }
}
